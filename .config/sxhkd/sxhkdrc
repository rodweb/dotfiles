XF86Audio{Play,Stop,Prev,Next}
	playerctl {play-pause,stop,previous,next}
XF86Audio{Raise,Lower}Volume
  vol {up,down}
XF86AudioMute
  vol mute

super + {space,e,r,t}
  {launcher,menu,launcher,sensible-terminal}

alt + ctrl + shift {a,s,d,f,g}
  {ror-tmux,discord,jumpapp -c emacs emacs,jumpapp -c google-chrome google-chrome-stable,jumpapp -c spotify spotify}

super + braceright
  bash -c 'killall xdotool || while xdotool click --repeat 100000 --delay 133 1; do :; done'
